{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "lib": ["es2017"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["*.ts"]
}
